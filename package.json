{
  "name": "ts-debounce-throttle",
  "version": "1.0.0",
  "main": "index.js",
  "author": "lifechat <lifechat@126.cn>",
  "homepage": "ts-debounce-throttle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifechat/ts-debounce-throttle.git"
  },
  "scripts": {
    "prepare": "husky install",
    "pretter": "prettier --write .",
    "build:clean": "rimraf dist/*",
    "test": "yarn run test:code && yarn run test:types",
    "test:code": "jest",
    "test:types": "tsc --noEmit && tsd",
    "prepublish": "yarn build",
    "lintstaged": "lint-staged",
    "build": "yarn build:clean && webpack --config webpack.common.js && tsc -p tsconfig.json --emitDeclarationOnly",
    "release": "np --no-yarn"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write ."
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.1.1",
    "all-contributors-cli": "^6.22.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "np": "^7.6.2",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsd": "^0.24.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "throttle",
    "typescript",
    "debounce"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "timers": "fake"
  },
  "tsd": {
    "directory": "src"
  }
}
